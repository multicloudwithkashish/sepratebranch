# app.py
# Simple Python program

def add(a, b):
    return a + b

def main():
    print("Welcome to Simple Python App")

    x = int(input("Enter first number:99"))
    y = int(input("Enter second number:99"))

    result = add(x, y)
    print("Sum is:", result)

if __name__ == "__main__":
    main()
